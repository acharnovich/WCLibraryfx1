<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane fx:id="searchRemovePane" prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1" fx:controller="Controller.AddOrRemoveItemCtrl">
   <children>
      <TableView fx:id="searchRemoveTbl" layoutX="-1.0" layoutY="47.0" prefHeight="352.0" prefWidth="600.0">
        <columns>
          <TableColumn fx:id="itemIdCol" prefWidth="75.0" text="Item ID" />
          <TableColumn fx:id="titleCol" prefWidth="75.0" text="Title" />
        </columns>
      </TableView>
      <Button fx:id="SearchRemoveItemBtn" disable="true" layoutX="185.0" layoutY="14.0" mnemonicParsing="false" text="Search" />
      <TextField fx:id="searchRemoveTxt" layoutX="14.0" layoutY="14.0" onKeyReleased="#removeItemVailidate" promptText="Item ID#" />
   </children>
</AnchorPane>
